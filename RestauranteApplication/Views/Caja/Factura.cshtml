@using RestauranteApplication.Models
<label>Factura: </label><p style="display: inline;">@ViewBag.id_factura</p>
<br>
<label>Cliente: </label><p style="display: inline;">@ViewBag.n_cliente</p>
<table>
    <tr>
        <th>Articulo</th>
        <th>Precio</th>
    </tr>
    @foreach (var producto in (List<productos>)Session["productos_caja"])
    {
        <tr>
            <td>
                @producto.nombre
            </td>
            <td>
                @producto.precio
            </td>
        </tr>
    }
    <tr>
        <td>Total</td>
        <td>@ViewBag.total</td>
    </tr>
    <tr>
        <td>Cambio</td>
        <td>@ViewBag.cambio</td>
    </tr>
</table>
@if (!Request.IsAuthenticated)
{
    Response.Redirect("~/");
}