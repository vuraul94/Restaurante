@model IEnumerable<RestauranteApplication.Models.ProveedorMulti>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="container">
    <h2>Proveedores</h2>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.proveedores.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.proveedores.direccion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.proveedores.email)
            </th>
            <th>telefono1</th>
            <th>telefono2</th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.proveedores.nombre)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.proveedores.direccion)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.proveedores.email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.telefono1.telefono)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.telefono2.telefono)
                </td>
                <td>
                    @Html.ActionLink("Modificar", "Edit", new { id = item.proveedores.id_proveedor }) |
                    @Html.ActionLink("Detalles", "Details", new { id = item.proveedores.id_proveedor }) |
                    @Html.ActionLink("Eliminar", "Delete", new { id = item.proveedores.id_proveedor })
                </td>
            </tr>
        }

    </table>
    </div>
    @if (!Request.IsAuthenticated)
    {
        Response.Redirect("~/");
    }
